// my code 

	import java.util.*;
	
	public class Main {
		public static void solution(String str) {
			
			String[] s = str.split(" "); // 입력 받은 문자열을 split 함수를 사용해 파싱.
			String s1 = s[0]; // 첫 문자열.
			char c[] = s[1].toCharArray();
			char c1 = c[0]; // 두번째 문자.
			
			char temp[] = s1.toCharArray(); // 문자열을 순회하면서 인덱스의 차이를 구하기 위해 문자 배열로 변환.
			for (int i = 0; i < temp.length; i++) { // i번째 문자와 c1과 같은 문자를 가진 인덱스의 차이를 계산하기 위해 이중 for문을 사용.
				int min = Integer.MAX_VALUE; // 문자열 내에 c1과 같은 문자가 여러개 있을 수 있기 때문에 최솟값을 변수에 담기 위한 min 변수.
				for (int j = 0; j < temp.length; j++) {
					if (c1 == temp[j] && Math.abs(i-j) < min) { // c1과 문자 배열의 문자가 같고, 인덱스의 차이가 최소일 때 min 변수에 인덱스의 차이를 저장.
						min = Math.abs(i-j);
					}
				}
				System.out.print(min + " ");
			}
		}
			
		public static void main(String[] args) {
			Scanner kb = new Scanner(System.in);
			String str = kb.nextLine();
			solution(str);
		}
	}
